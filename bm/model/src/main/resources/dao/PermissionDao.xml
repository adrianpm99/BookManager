<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
    xmlns="http://www.ontimize.com/schema/jdbc"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
    catalog="" schema="${mainschema}" table="TROLE"
    datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>ID_ROLENAME</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>ID_ROLENAME</Column>
    </UpdateKeys>
    <GeneratedKey>ID_ROLENAME</GeneratedKey>
    <Queries>
	    <Query id="clientPermissions">
	    <AmbiguousColumns>
	    	<AmbiguousColumn name="ID_ROLENAME" prefix="R" databaseName="ID_ROLENAME"/>
	    	<AmbiguousColumn name="UR_ID_ROLENAME" prefix="UR" databaseName="ID_ROLENAME"/> 	
	    </AmbiguousColumns>
			<Sentence>
			<![CDATA[
				select
					#COLUMNS#
				from
					TROLE R
					INNER JOIN TUSER_ROLE UR
					ON R.ID_ROLENAME = UR.ID_ROLENAME
				#WHERE#
			]]>
			</Sentence>
		</Query>
    </Queries>
</JdbcEntitySetup>